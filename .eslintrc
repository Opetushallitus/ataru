{
  "extends": ["prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier", "prefer-arrow"],
  "rules": {
    "prettier/prettier": ["error"],
    "prefer-arrow/prefer-arrow-functions": "error",
    "array-callback-return": "off",
    "prefer-const": "error",
    "no-var": "error"
  },
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
